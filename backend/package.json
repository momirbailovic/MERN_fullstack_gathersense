{
  "name": "gathersense",
  "version": "1.0.0",
  "description": "gathersense on node",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon ./app.js",
    "stop": "lsof -ti :5000 | xargs kill",
    "mongo:start": "docker run --name gathersense-mongo -p 27017:27017 mongo & sleep 5",
    "mongo:stop": "docker stop gathersense-mongo && docker rm gathersense-mongo"
  },
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "bluebird": "^3.7.1",
    "body-parser": "1.15.0",
    "bscript": "^0.1.0",
    "cors": "2.7.1",
    "dotenv": "^8.1.0",
    "ejs": "2.4.1",
    "errorhandler": "1.4.3",
    "express": "4.13.4",
    "express-jwt": "3.3.0",
    "express-session": "1.13.0",
    "file-system": "^2.2.2",
    "gmail-send": "^1.8.10",
    "jsonwebtoken": "7.1.9",
    "method-override": "2.3.5",
    "methods": "1.1.2",
    "mongoose": "4.4.10",
    "mongoose-unique-validator": "1.0.2",
    "morgan": "1.7.0",
    "multer": "^1.4.2",
    "multiparty": "^4.2.1",
    "nodemailer": "^6.3.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "promise": "^8.0.3",
    "request": "2.69.0",
    "sendgrid": "^1.5.0",
    "slug": "0.9.1",
    "underscore": "1.8.3",
    "wait-until": "0.0.2"
  },
  "devDependencies": {
    "newman": "^3.8.2",
    "nodemon": "^1.11.0"
  }
}
